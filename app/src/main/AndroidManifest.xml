            android:screenOrientation="portrait" />



        <activity
            android:name="com.flyingeffects.com.ui.view.activity.LoginActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait" />


        <activity
            android:name="com.flyingeffects.com.ui.view.activity.RegiestActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait" />


        <activity
            android:name="com.flyingeffects.com.ui.view.activity.ForgetActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.flyingeffects.com.ui.view.activity.PreviewActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait" />


        <activity
            android:name="com.flyingeffects.com.ui.view.activity.AboutActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.flyingeffects.com.ui.view.activity.webViewActivity"
            android:configChanges="orientation|keyboardHidden|screenSize"
